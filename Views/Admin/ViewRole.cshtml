@model IEnumerable<B13_MS_School.Models.List>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Actorid)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Actorid)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-email="@item.Email">Edit</a> |
                    <a asp-action="Details" asp-route-email="@item.Email">Details</a> |
                    <a asp-action="Delete" asp-route-email="@item.Email">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>